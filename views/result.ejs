<!DOCTYPE html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <title>体质调查问卷</title>
    <link rel='stylesheet prefetch' href='https://cdn.bootcss.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link rel='stylesheet prefetch' href='css/animate.min.css'>
    <link rel="stylesheet" type="text/css" href="css/default.css">
    <link rel='stylesheet'
        href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,100,400,300,600,700,800'>
    <link rel="stylesheet" type="text/css" href="css/styles.css">
    <link rel="stylesheet" type="text/css" href="css/input_radio.css">

</head>

<body>
    <div class="container-fluid table-class">
    <div class="black">
        <div class="inner cover">
            <div class="centerUp">
                <div class="questionaire">
                    <table class="table-bordered" style="width:100%;text-align:center;">
                        <caption>检测结果</caption>
                        <thead>
                            <tr>
                                <th style="word-break: keep-all;text-align:center;">体质名称</th>
                                <th style="word-break: keep-all;text-align:center;">基本是(倾向是)</th>
                                <th style="word-break: keep-all;text-align:center;">是</th>
                                <th style="word-break: keep-all;text-align:center;">否</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i in question_list) { %>
                            <tr>
                                <td><%= question_list[i][4] %></td>
                                <td>
                                    <% if (question_list[i][3]==2) { %>
                                    <span class="glyphicon glyphicon-ok"></span>
                                    <% } else { %>
                                    <span></span>
                                    <% } %>
                                </td>
                                <td>
                                    <% if (question_list[i][3]==1) { %>
                                    <span class="glyphicon glyphicon-ok"></span>
                                    <% } else { %>
                                    <span></span>
                                    <% } %>
                                </td>
                                <td>
                                    <% if (question_list[i][3]==0) { %>
                                    <span class="glyphicon glyphicon-ok"></span>
                                    <% } else { %>
                                    <span></span>
                                    <% } %>
                                </td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
                <p><%= mydata %></p>
                <div id="chart1" style="width: 1024px;height:400px;"></div>
            </div>
        </div>
    </div>
    </div>
    <script src="js/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
    <script src='https://cdn.bootcss.com/bootstrap/3.2.0/js/bootstrap.min.js'></script>
    <script src="js/echarts.min.js"></script>
    <script type="text/javascript">
        var myChart = echarts.init(document.getElementById('chart1'));

        // 指定图表的配置项和数据
        var option = {
            title : {
                text: '流调数据提交医院排名',
                subtext: '排卵障碍性异常子宫出血',
                x:'center'
            },
            tooltip : {
                trigger: 'axis',
                axisPointer : {            // 坐标轴指示器，坐标轴触发有效
                    type : 'shadow'        // 默认为直线，可选为：'line' | 'shadow'
                }
            },
            legend: {
                orient : 'vertical',
                x : 'left',
                data:['未审核','审核通过','审核不通过']
            },
            calculable : true,
            xAxis : [
                {
                    type : 'category',
                    //设置字体倾斜  
                    axisLabel: {
                        interval: 0,
                        rotate: 45,
                        //倾斜度 -90 至 90 默认为0  
                        margin: 2,
                        textStyle: {
                            //fontWeight: "bolder",
                            color: "#000000"
                        }
                    },
                    data : ['广安门中医院','东直门中医院','北京中医院','河北中医院','杭州市中医院','云南中医药大学第一附属医院','广东省中医院']
                }
            ],
            yAxis : [
                {
                    type : 'value'
                }
            ],
            series : [
                {
                    name:'未审核',
                    type:'bar',
                    stack:'hospital',
                    data:[400, 390, 350, 346, 330, 325, 320]
                },
                {
                    name:'审核通过',
                    type:'bar',
                    stack: 'hospital',
                    data:[200, 200, 190, 186, 100, 98, 96]
                },
                {
                    name:'审核不通过',
                    type:'bar',
                    stack: 'hospital',
                    //设置柱的宽度，要是数据太少，柱子太宽不美观~
                    barWidth:70,
                    data:[56, 60, 50, 50, 60, 65, 30]
                }
            ]
        };

        // 使用刚指定的配置项和数据显示图表。
        myChart.setOption(option);
    </script>
</body>